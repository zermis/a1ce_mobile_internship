{
    "name": "a1ce-mobile",
    "version": "1.0.0",
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "test": "jest -u",
        "appium": "appium",
        "e2e": "npx wdio ./e2e/wdio.conf.js",
        "lint": "eslint --ext .tsx --ext .ts src/",
        "lint:fix": "npx eslint ./src/**/*.tsx --fix",
        "prettier:write": "npx prettier --write **/*.{js,jsx,ts,tsx,json} && npx prettier --write *.{js,jsx,ts,tsx,json}",
        "prettier:watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
        "prettier-lint:fix": "npx prettier --write **/*.{js,jsx,ts,tsx,json} && npx prettier --write *.{js,jsx,ts,tsx,json} && npx eslint ./src/**/*.tsx --fix",
        "build:android": "expo build:android --type apk",
        "build:android-on-pipeline": "expo build:android --type apk --non-interactive --no-wait"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged && npm run lint"
        }
    },
